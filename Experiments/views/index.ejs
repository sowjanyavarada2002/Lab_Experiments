<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <table>
      <tr>
        <th>PostTitle</th>
        <th>message</th>
      </tr>
      <% posts.forEach((post)=>{ %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.message %></td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
